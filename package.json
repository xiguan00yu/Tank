{
    "name": "tank",
    "version": "1.0.0",
    "description": "robot tank",
    "main": "index.js",
    "scripts": {
        "start:dev": "NODE_ENV=development npm-run-all server:dev",
        "start:prod": "npm run prod",
        "server:dev": "babel-node ./src/index.js",
        "server:prod": "node ./dist/index.js",
        "prod": "NODE_ENV=production npm-run-all clean build server:prod",
        "clean": "rimraf dist",
        "build": "babel ./src --out-dir dist"
    },
    "author": "XiaoYu",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.4.4",
        "npm-run-all": "^4.1.5",
        "rimraf": "^2.6.3"
    },
    "dependencies": {}
}
